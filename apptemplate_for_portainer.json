{
	"version": "2",
	"templates": [
        {
			"type": 1,
			"title": "Influxdb 2.x only for x86",
			"description": "Time series database.",
			"categories": [
				"Database"
			],
			"platform": "linux",
			"logo": "",
			"image": "influxdb:latest",
			"env": [{
					"name": "DOCKER_INFLUXDB_INIT_USERNAME",
					"label": "Username",
					"default": "admin"
				},
				{
					"name": "DOCKER_INFLUXDB_INIT_PASSWORD",
					"label": "Password",
					"default": "admin"
				},
				{
					"name": "DOCKER_INFLUXDB_INIT_ORG",
					"label": "Organisation",
					"default": "costume"
				},
                {
					"name": "DOCKER_INFLUXDB_INIT_BUCKET",
					"label": "Database name",
					"default": "my-bucket"
				}
			],
			"ports": [
				"8086:8086"
			],
			"volumes": [{
					"container": "/root/influxdb/data"
				},
				{
					"container": "/root/influxdb2/data"
				}
			]
		},
		{
			"type": 1,
			"title": "Influxdb 1.x for ARM and x86",
			"description": "Time series database.",
			"categories": [
				"Database"
			],
			"platform": "linux",
			"logo": "",
			"image": "influxdb:1.8.5-alpine",
			"env": [{
				"name": "DOCKER_INFLUXDB_INIT_USERNAME",
				"label": "Username",
				"default": "admin"
			},
			{
				"name": "DOCKER_INFLUXDB_INIT_PASSWORD",
				"label": "Password",
				"default": "admin"
			},
			{
				"name": "DOCKER_INFLUXDB_INIT_ORG",
				"label": "Organisation",
				"default": "costume"
			},
			{
				"name": "DOCKER_INFLUXDB_INIT_BUCKET",
				"label": "Database name",
				"default": "my-bucket"
			}
			],
			"ports": [
				"8086:8086"
			],
			"volumes": [{
					"container": "/root/influxdb/data"
				},
				{
					"container": "/root/influxdb2/data"
				}
			]
		},
        {
			"type": 1,
			"title": "Chronograph",
			"description": "Chronograf is a visualization tool for time series data in InfluxDB.",
			"categories": [
				"Visu"
			],
			"platform": "linux",
			"logo": "",
			"image": "chronograf:latest",
			"ports": [
				"8888:8888"
			],
			"volumes": [{
					"container": "/var/lib/chronograf"
				}
			]
		},
        {
			"type": 1,
			"title": "Grafana",
			"description": "The open-source platform for monitoring and observability.",
			"categories": [
				"Visu"
			],
			"platform": "linux",
			"logo": "",
			"image": "grafana/grafana:latest",
			"ports": [
				"3000:3000"
			],
			"volumes": [{
					"container": "/var/lib/grafana"
				}
			]
		},
        {
			"type": 1,
			"title": "Pi-Hole",
			"description": "Network-wide ad blocking via your own Linux hardware",
			"categories": [
				"Addons"
			],
			"platform": "linux",
			"logo": "",
			"image": "pihole/pihole",
			"env": [{
					"name": "TZ",
					"label": "Timezone",
					"default": "Europe/Berlin"
				},
				{
					"name": "WEBPASSWORD",
					"label": "Password for Webinterface",
					"default": "admin"
				}
			],
			"ports": [
                "53:53/tcp",
                "53:53/udp",
                "67:67/udp",
                "8080:80/tcp"
			],
			"volumes": [{
					"container": "/etc/pihole/"
				},
				{
					"container": "/etc/dnsmasq.d/"
				}
            ],    
            "capabilities":[
                "NET_ADMIN"
            ]
		},
        {
			"type": 1,
			"title": "Node-Red",
			"description": "Low-code programming for event-driven applications.",
			"categories": [
				"Addons"
			],
			"platform": "linux",
			"logo": "",
			"image": "nodered/node-red",
			"env": [{
					"name": "TZ",
					"label": "Timezone",
					"default": "Europe/Berlin"
				}
			],
			"ports": [
                "1880:1880"
			],
			"volumes": [{
					"container": "/data"
				}
            ],    
            "capabilities":[
                "NET_ADMIN"
            ]
		},
		{
			"type": 1,
			"title": "iobroker",
			"description": "ioBroker is an integration platform for the Internet of Things, focused on Building Automation, Smart Metering, Ambient Assisted Living, Process Automation, Visualization and Data Logging.",
			"categories": [
				"Addons"
			],
			"platform": "linux",
			"logo": "",
			"image": "buanet/iobroker:latest",
			"env": [{
					"name": "TZ",
					"label": "Timezone",
					"default": "Europe/Berlin"
				}
			],
			"ports": [
				"8081:8081"
			],
			"volumes": [{
					"container": "/opt/iobroker"
				}
			]
		},
		{
			"type": 1,
			"title": "Home Assitant",
			"description": "Open source home automation that puts local control and privacy first. Powered by a worldwide community of tinkerers and DIY enthusiasts.",
			"categories": [
				"Addons"
			],
			"platform": "linux",
			"logo": "",
			"image": "homeassistant/home-assistant:stable",
			"ports": [
				"8123:8123"
			],
			"volumes": [{
					"container": "/etc/localtime",
					"bind": "/etc/localtime",
					"readonly": true
				},
				{
					"container": "/config"
				}
			],	
			"network": "host"
		},
		{
			"type": 1,
			"title": "Adguard",
			"description": "Free and open source, powerful network-wide ads & trackers blocking DNS server.",
			"categories": [
				"Addons"
			],
			"platform": "linux",
			"logo": "",
			"image": "adguard/adguardhome:latest",
			"ports": [
				"80/tcp",
				"443/tcp",
                "53:53/tcp",
                "53:53/udp",
                "67:67/udp",
				"68:68/udp",
				"68:68/tcp",
				"853:853/tcp",
				"784:784/udp",
				"3000:3000/tcp",
				"5443:5443/tcp"
			],
			"volumes": [{
					"container": "/opt/adguardhome/work"
				},
				{
					"container": "/opt/adguardhome/conf"
				}
			]
		},
		{
			"type": 1,
			"title": "Eclipse Mosquitto",
			"description": "Eclipse Mosquitto is an open source message broker which implements MQTT version 5, 3.1.1 and 3.1",
			"categories": [
				"Addons"
			],
			"platform": "linux",
			"logo": "",
			"image": "eclipse-mosquitto:latest",
			"ports": [
                "1883:1883",
                "9001:9001"
			],
			"volumes": [{
					"container": "/mosquitto/config/"
				},
				{
					"container": "/mosquitto/data/"
				},
				{
					"container": "/mosquitto/log/"
				}
			]
		}
	]
}